{
  "name": "guacamole-tests",
  "license": "Apache-2.0",
  "workspaces": [
    "modules/*"
  ],
  "scripts": {
    "commitlint": "commitlint --edit",
    "format": "prettier --write package.json \"modules/**/*.ts\"",
    "lint": "eslint package.json \"modules/**/*.ts\" --fix",
    "lint:ci": "eslint package.json \"modules/**/*.ts\" --max-warnings=0",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "prepare": "husky install .github/hooks"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/jest": "^29.5.8",
    "@types/node": "~20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "esbuild": "^0.19.5",
    "eslint": "~8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "typescript": "^5.3.2"
  }
}
